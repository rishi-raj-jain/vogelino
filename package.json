{
  "private": true,
  "scripts": {
    "dev": "TAILWIND_MODE=watch astro dev",
    "start": "astro dev",
    "build": "npm run downloadCacheData && npm run downloadInspiration && npm run downloadCollaborators && npm run downloadProjectImages && npm run astroBuild",
    "astroBuild": "npm run type-check && npm run format && npm run lint && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --noEmit",
    "downloadCacheData": "npx ts-node -P scripts/tsconfig.json scripts/downloadCacheData.ts",
    "downloadInspiration": "npx ts-node -P scripts/tsconfig.json scripts/downloadNotionInspirationImages.ts",
    "downloadCollaborators": "npx ts-node -P scripts/tsconfig.json scripts/downloadNotionCollaboratorsImages.ts",
    "downloadProjectImages": "npx ts-node -P scripts/tsconfig.json scripts/downloadNotionProjectImages.ts",
    "downloadDrawings": "npx ts-node -P scripts/tsconfig.json scripts/downloadNotionDrawings.ts",
    "lint": "eslint 'src/**/*.{js,ts,astro}'",
    "lint-fix": "npm run lint -- --fix",
    "format": "npx prettier --write src"
  },
  "devDependencies": {
    "@astrojs/prefetch": "0.2.3",
    "@astrojs/sitemap": "1.3.3",
    "@astrojs/tailwind": "3.0.0",
    "@astrojs/ts-plugin": "1.0.10",
    "@notionhq/client": "2.2.0",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/marked": "4.0.7",
    "@types/puppeteer": "5.4.6",
    "@types/sharp": "0.30.5",
    "@typescript-eslint/parser": "5.61.0",
    "astro": "2.7.1",
    "autoprefixer": "10.4.14",
    "dotenv": "16.0.2",
    "esbuild": "0.15.7",
    "eslint": "8.44.0",
    "eslint-plugin-astro": "0.27.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "node-fetch": "2.6.7",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "0.10.0",
    "puppeteer": "14.1.1",
    "sharp": "0.31.0",
    "slugify": "1.6.5",
    "tailwindcss": "3.3.2",
    "tailwindcss-touch": "1.0.1",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@splidejs/splide": "4.1.4",
    "@vercel/analytics": "1.0.1",
    "astro-imagetools": "0.6.11"
  }
}
